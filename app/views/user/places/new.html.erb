<p id="notice"></p>
 <div class="container">
  <div class="row">
   <div class ="mx-auto"> 
    <h3>待ち合わせ新規投稿</h3>
   
     <%= form_with model: @place do |f| %>
       <h4>件名</h4>
        <%= f.text_field :subject %>
       <h4>本文</h4>
        <%= f.text_area :body, size: "80x10" %>
       <h4>待ち合わせ場所</h4>
        <%= f.text_field :address %>
       <h4>待ち合わせ目印</h4>
        <%= f.text_field :spot %>
       <h4>待ち合わせ日時</h4>
        <%= f.datetime_field :place_at, use_month_numbers: true, class: 'form-control' %>
       <%= f.submit '投稿' %>
     <% end %>
   </div>
  </div>
 </div>
